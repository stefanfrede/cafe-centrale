	"engines": {
		"node": ">=22",
		"npm": ">=10"
	},
	"description": "The website of Caf√© Centrale, an Italian bistro in Hamburg Ottensen.",
	"type": "module",
	"scripts": {
		"prebuild": "run-s clean",
		"build": "eleventy",
		"postbuild": "rimraf _dist/* && cp -a _site/. _dist/",
		"build-ghpages": "npx @11ty/eleventy --pathprefix=/cafe-centrale/",
		"prestart": "run-s clean",
		"start": "eleventy --serve --incremental --quiet",
		"clean": "rimraf _site",
		"debug": "cross-env DEBUG=Eleventy* npx @11ty/eleventy",
		"benchmark": "cross-env DEBUG=Eleventy:Benchmark* npx @11ty/eleventy"
	},
	"repository": {
